echo "script takes 2 args PLAYLIST_URL   DIR_NAME  "

if [ $2 ]
then



x=$1
curl "$x"|grep -i '?v='|cut -d'&' -f1|cut -d? -f2|cut -d= -f2|grep -v title|grep -v class|grep -v set|grep -v -i was|grep -v video|uniq > /tmp/p

mkdir $2
cd $2
for i in `cat /tmp/p`;do youtube-dl https://www.youtube.com/watch?v=$i ;done
fi
